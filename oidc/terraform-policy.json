{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "eks:*",
        "ec2:*",
        "s3:*",
        "iam:PassRole",
        "iam:GetUser",
        "secretsmanager:GetSecretValue",
        "sts:AssumeRole",
        "dynamodb:PutItem",
        "dynamodb:GetItem",
        "dynamodb:DeleteItem"
      ],
      "Resource": "*"
    }
  ]
}